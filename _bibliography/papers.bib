  keywords = "PTHrP; TRPA1; TRPV1; TRPV4; cancer pain; mechanical pain; pain",
  language = "en"
}

@ARTICLE{Noh2018-fj,
  title    = "Miniaturized, battery-free optofluidic systems with potential for
              wireless pharmacology and optogenetics",
  author   = "Noh, Kyung Nim and Park, Sung Il and Qazi, Raza and Zou, Zhanan
