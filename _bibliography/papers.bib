@ARTICLE{Mickle2016-zz,
  title    = "Nociceptive {TRP} channels: Sensory detectors and transducers in
              multiple pain pathologies",
  author   = "Mickle, Aaron D and Shepherd, Andrew J and Mohapatra, Durga P",
  abstract = "Specialized receptors belonging to the transient receptor
              potential (TRP) family of ligand-gated ion channels constitute
